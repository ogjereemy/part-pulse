{
  "expo": {
    "name": "Rave",
    "slug": "Rave",
    "version": "1.0.0",
    "orientation": "portrait",

    "scheme": "partypulse",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },

    "web": {
      "output": "static"

    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {

          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
